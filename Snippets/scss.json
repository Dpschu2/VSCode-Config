	"border-image": {
		"prefix": ["bi", "borderimage"],
		"body": "border-image: linear-gradient(to right, stencilColor(color-grey2), rgba(stencilColor(color-grey2), 0.8)) fill 1;",
		"description": "border-image"
	},
	"animate-appear": {
		"prefix": ["fade-animate", "fa"],
		"body": "@supports (animation-timeline: view()) {\n    @media (prefers-reduced-motion: no-preference) {\n        @keyframes appear {\n            from {\n                opacity: 0;\n                scale: 0.8;\n            }\n            to {\n                opacity: 1;\n                scale: 1;\n            }\n        }\n        .animate-appear {\n            animation: appear linear;\n            animation-timeline: view();\n            animation-range: entry 0% cover 50%;\n        }\n    }\n}",
		"description": "animate-appear"
	},
	"animate-scroll": {
		"prefix": ["sa", "scroll-animate"],
		"body": "@supports (animation-timeline: view()) {\n    @media (prefers-reduced-motion: no-preference) {\n        @keyframes appear {\n            from {\n                opacity: 0;\n                scale: 0.8;\n            }\n            to {\n                opacity: 1;\n                scale: 1;\n            }\n        }\n        .animate-appear {\n            animation: appear linear;\n            animation-timeline: view();\n            animation-range: entry 0% cover 50%;\n        }\n    }\n}",
		"description": "animate-scroll"
	},
	"accordion-animate": {
		"prefix": ["accordion-animate"],
		"body": ".content {\n  display: grid;\n  grid-template-rows: 0fr;\n  transition: grid-template-rows 0.5s ease-in-out;\n  overflow: hidden;\n\n  &.open {\n    grid-template-rows: 1fr;\n  }\n\n  .inner {\n    min-height: 0;\n  }\n}",
		"description": "accordion-animate"
	},
	"fade": {
		"prefix": ["fade"],
		"body": "transition: display 0.3s allow-discrete, overlay 0.3s allow-discrete, opacity 0.3s;\nopacity: 0;\n\n&[open] {\n\topacity: 1;\n\n\t@starting-style {\n\t\topacity: 0;\n\t}\n}",
		"description": "fade effect"
	},
	"anchor": {
		"prefix": ["anchor"],
		"body": ".map-marker {\n    anchor-name: --my-anchor;\n}\n.marker-popup {\n    position: absolute;\n    top: anchor(--my-anchor bottom);\n    justify-self: anchor-center;\n}",
		"description": "anchor"
	},
	"sr-only": {
		"prefix": ["sr"],
		"body": ".sr-only:not(:focus):not(:active) {\n    clip: rect(0 0 0 0); \n    clip-path: inset(50%);\n    height: 1px;\n    overflow: hidden;\n    position: absolute;\n    white-space: nowrap; \n    width: 1px;\n}",
		"description": "sr-only"
	},
	"select dropdown": {
		"prefix": ["select-dropdown", "sel"],
		"body": ".form-field:has(.form-select) {\n    position: relative;\n    display: inline-block;\n    \n    &:after {\n        content: url(\"data:image/svg+xml;charset=UTF-8,%3csvg width='12' height='8' viewBox='0 0 12 8' xmlns='http://www.w3.org/2000/svg'%3e%3cpath d='M6.00005 7.4748C5.86672 7.4748 5.74172 7.45397 5.62505 7.4123C5.50838 7.37064 5.40005 7.2998 5.30005 7.1998L0.700049 2.5998C0.516715 2.41647 0.425049 2.18314 0.425049 1.8998C0.425049 1.61647 0.516715 1.38314 0.700049 1.1998C0.883382 1.01647 1.11672 0.924805 1.40005 0.924805C1.68338 0.924805 1.91672 1.01647 2.10005 1.1998L6.00005 5.0998L9.90005 1.1998C10.0834 1.01647 10.3167 0.924805 10.6 0.924805C10.8834 0.924805 11.1167 1.01647 11.3 1.1998C11.4834 1.38314 11.575 1.61647 11.575 1.8998C11.575 2.18314 11.4834 2.41647 11.3 2.5998L6.70005 7.1998C6.60005 7.2998 6.49172 7.37064 6.37505 7.4123C6.25838 7.45397 6.13338 7.4748 6.00005 7.4748Z'/%3e%3c/svg%3e\");\n        position: absolute;\n        right: 10px;\n        bottom: 0;\n        transform: translateY(-50%);\n        height: 18px;\n        width: 18px;\n        z-index: 1;\n        pointer-events: none;\n    }\n    .form-select {\n        min-width: 230px;\n        background: stencilColor(color-white);\n        appearance: none;\n        -webkit-appearance: none;\n        -moz-appearance: none;\n    }\n}",
		"description": "select dropdown"
	}
